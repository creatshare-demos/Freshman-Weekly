//二叉树的创建，以及三种遍历递归算法
#include<stdio.h>
#include<stdlib.h>
#include<string.h> 
typedef struct Node
{
	char date;
	struct Node *lchild, *rchild;
}BiNode, *BiTree;

int CreateTiTree(BiTree T)
{
	char ch;
	scanf("%c", &ch);
	if(ch=='*')
		T=NULL;
	else{
		T=(BiTree)malloc(sizeof(BiNode));
		T->date=ch;
		CreateTiTree(T->lchild);
		CreateTiTree(T->rchild);
	}
	return 0;
}

int PreOrder(BiTree T)
{
	if(T){
		printf("%c ", T->date);
		PreOrder(T->lchild);
		PreOrder(T->rchild);
	}
	return 0;
}

int main()
{
	BiTree T;
	CreateTiTree(T);
	PreOrder(T);
	return 0;
}
